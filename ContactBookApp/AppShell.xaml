<?xml version="1.0" encoding="UTF-8" ?>
<Shell x:Class="ContactBookApp.AppShell"
       xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
       xmlns:local="clr-namespace:ContactBookApp"
       xmlns:view="clr-namespace:ContactBookApp.View"
       FlyoutBackgroundColor="{AppThemeBinding Light={StaticResource ModusBlue}, Dark={StaticResource ModusGraySecondry}}"
       Title="ContactBookApp">

    <FlyoutItem Title="CollectionView">
        <ShellContent ContentTemplate="{DataTemplate local:MainPage}"
                      Route="MainPage" />
    </FlyoutItem>
    <FlyoutItem Title="Listview">
        <ShellContent ContentTemplate="{DataTemplate view:MainPageList}"
                      Route="MainPagelist" />
    </FlyoutItem>

</Shell>
